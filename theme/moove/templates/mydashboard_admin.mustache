<div class="admininfos">
    {{{ output.mydashboard_admin_header }}}
    <div class="row mx-4 mt-4">
        <h1>Welcome,</h1>
    </div>
    <div class="row mx-4">
        <h1>{{{user.firstname}}} !</h1>
    </div>
    <div class="row justify-content-center mx-0 my-2" id="row-dashboard-admin">
      <div class="col">
        <!-- small box -->
        <div class="small-box bg-1">
          <div class="inner">
            <h2>Import User</h2>
            <i class="slicon-cloud-upload icon"></i>
          </div>
        </div>
      </div>

      <div class="col">
        <!-- small box -->
        <div class="small-box bg-2">
          <div class="inner">
            <h2>Memberi Nilai</h2>
            <h4>Tahap 1 Main Event</h4>
            <i class="slicon-check icon"></i>
          </div>
        </div>
      </div>

      <div class="col">
        <!-- small box -->
        <div class="small-box bg-3">
          <div class="inner">
            <h2>Download Submission</h2>
            <h4>Siklus Akuntansi</h4>
            <i class="slicon-cloud-download icon"></i>
          </div>
        </div>
      </div>
  </div>
  <div class="row justify-content-center" style="width: 100%;">
      <div class="col-xl-3 col-12">
        <!-- small box -->
        <div class="small-box bg-4">
          <div class="inner">
            <h2>Pengaturan Preliminary Event</h2>
          </div>
          <div class="icon">
            <i class="slicon-settings"></i>
          </div>
        </div>
      </div>

      <div class="col-xl-3 col-12">
        <!-- small box -->
        <div class="small-box bg-4">
          <div class="inner">
            <h2>Pengaturan Main Event</h2>
          </div>
          <div class="icon">
            <i class="slicon-settings"></i>
          </div>
        </div>
      </div>
  </div>
</div>
